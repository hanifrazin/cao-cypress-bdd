{
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cypress/webpack-preprocessor": "^6.0.4",
    "cypress": "^14.3.3",
    "cypress-xpath": "^2.0.1",
    "esbuild": "^0.25.4",
    "mochawesome": "^7.1.3"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1"
  },
  "name": "cao-cypress-bdd",
  "version": "1.0.0",
  "description": "",
  "main": "cypress.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bdd": "npx cypress run --browser chrome --headless --spec \"cypress/e2e/features/*.feature\"",
    "bdd:login": "npx cypress run --browser chrome --headless --spec \"cypress/e2e/features/login.feature\"",
    "bdd:cart": "npx cypress run --browser chrome --headless --spec \"cypress/e2e/features/shopping_cart.feature\"",
    "bdd:checkout": "npx cypress run --browser chrome --headless --spec \"cypress/e2e/features/checkout.feature\"",
    "merge:report": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/merged.json",
    "generate:report": "npx merge cypress/reports/merged.json --reportDir cypress/reports/html",
    "test:report": "npm run bdd && npm run merge:report && npm run generate:report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanifrazin/cao-cypress-bdd.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/hanifrazin/cao-cypress-bdd/issues"
  },
  "homepage": "https://github.com/hanifrazin/cao-cypress-bdd#readme"
}
